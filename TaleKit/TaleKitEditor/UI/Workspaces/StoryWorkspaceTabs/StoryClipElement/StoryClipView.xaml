<UserControl x:Class="TaleKitEditor.UI.Workspaces.StoryWorkspaceTabs.StoryClipElements.StoryClipView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 

			 xmlns:StoryWorkspaceTabs="clr-namespace:TaleKitEditor.UI.Workspaces.StoryWorkspaceTabs"
			 xmlns:GControls="clr-namespace:GKitForWPF.UI.Controls;assembly=GKitForWPF"

             mc:Ignorable="d" 
             d:DesignWidth="220">
	<DockPanel Name="ItemPanel" Height="40" Background="{StaticResource ItemBackground}">
		<StackPanel Name="OrderIndicatorContext" DockPanel.Dock="Right" VerticalAlignment="Center" Orientation="Horizontal" Margin="8,0,8,0">
		</StackPanel>
		<GControls:EditTextBlock x:Name="NameEditText" Text="ClipName" Foreground="#FFB5B5B5" Margin="8,0,0,0" VerticalContentAlignment="Center"/>
	</DockPanel>
</UserControl>
