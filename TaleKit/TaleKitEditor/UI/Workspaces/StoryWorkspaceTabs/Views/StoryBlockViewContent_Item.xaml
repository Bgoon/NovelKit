<UserControl x:Class="TaleKitEditor.UI.Workspaces.StoryWorkspaceTabs.Views.StoryBlockViewContent_Item"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 

			 xmlns:VectorImages="clr-namespace:TaleKitEditor.Resources.VectorImages"

             mc:Ignorable="d" 
             d:DesignWidth="192" d:DesignHeight="40">
	<DockPanel>
		<Grid Name="PassTriggerIconContext" DockPanel.Dock="Right" Width="14" Margin="0,0,4,0">
		</Grid>
		<StackPanel Name="OrderIndicatorContext" DockPanel.Dock="Right" VerticalAlignment="Center" Orientation="Horizontal" Margin="4,0">
		</StackPanel>
		<TextBlock Name="PreviewTextBlock" Style="{StaticResource ContentText}" Text="스크립트 미리보기" VerticalAlignment="Center" Foreground="#FFB5B5B5" Margin="4,0,0,0"></TextBlock>
	</DockPanel>
</UserControl>
